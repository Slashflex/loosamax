{
  "name": "loosamax",
  "version": "1.0.0",
  "description": "Simple poker game (WIP)",
  "main": "app.js",
  "scripts": {
    "watch:sass": "node-sass public/sass/main.scss public/css/styles.css -w",
    "compile:sass": "node-sass public/sass/main.scss public/css/styles.comp.css",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' public/css/styles.comp.css -o public/css/styles.prefix.css",
    "compress:css": "node-sass public/css/styles.prefix.css public/css/styles.css --output-style compressed",
    "build:css": "npm-run-all compile:sass prefix:css compress:css",
    "start": "NODE_ENV=production node server",
    "dev": "nodemon server.js",
    "webpack": "webpack --config webpack.config.js"
  },
  "author": "Slashflex",
  "license": "ISC",
  "dependencies": {
    "@babel/polyfill": "^7.10.1",
    "axios": "^0.21.1",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-mongo-sanitize": "^2.0.0",
    "express-rate-limit": "^5.1.3",
    "helmet": "^3.23.1",
    "hpp": "^0.2.3",
    "html-to-text": "^5.1.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.19",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "ndb": "^1.1.5",
    "nodemailer": "^6.4.8",
    "pug": "^3.0.1",
    "sharp": "^0.25.4",
    "validator": "^13.1.1",
    "xss-clean": "^0.1.1"
  },
  "devDependencies": {
    "autoprefixer": "^10.2.5",
    "css-loader": "^5.2.4",
    "file-loader": "^6.0.0",
    "img-loader": "^3.0.1",
    "node-sass": "^7.0.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^8.3.1",
    "prettier": "^2.0.5",
    "source-map-loader": "^1.0.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  }
}
